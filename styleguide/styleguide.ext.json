{
  "ignore": [
    "**/__tests__/**",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/*.d.ts",
    "**/src/index.js",
    "**/src/Article/index.js",
    "**/src/Fa/FaStacked.js",
    "**/src/Footer/index.js",
    "**/src/Label/index.js",
    "**/src/Modal/Fade.js",
    "**/src/Option/index.js",
    "**/src/Output/index.js",
    "**/src/Section/index.js",
    "**/src/TetherContent/index.js",
    "**/A/composeLink.js",
    "**/theme/**",
    "**/theme.js"
  ],
  "ribbon": {
    "url": "https://github.com/bootstrap-styled/v4",
    "text": "Fork us on GitHub"
  },
  "pagePerSection": true,
  "sections": [
    {
      "name": "Introduction",
      "content": "docs/introduction.md"
    },
    {
      "name": "Documentation",
      "sections": [
        {
          "name": "Prerequisite",
          "content": "docs/prerequisite.md"
        },
        {
          "name": "Installation",
          "content": "docs/installation.md",
          "description": "Discover how to install `bootstrap-styled`"
        },
        {
          "name": "Configuration",
          "content": "docs/configuration.md"
        }
      ],
      "sectionDepth": 0
    },
    {
      "name": "UI Components",
      "components": "src/**/*.js",
      "ignore": [
        "src/Accordions/index.js",
        "src/Cards/index.js"
      ],
      "sectionDepth": 1
    },
    {
      "name": "Developers",
      "content": "docs/commands.md"
    }
  ]
}
